<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >


    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape>
            <stroke
                android:dashGap="2dp"
                android:dashWidth="6dp"
                android:width="1dp"
                android:color="@color/colorBlack" />
            <solid android:color="#D3D3D3" />

            <corners android:radius="5dp" />
        </shape>
    </item>

</layer-list>